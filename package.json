{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.8.1",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./src/main-process/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeitIG/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KeitIG/museeks/issues"
  },
  "scripts": {
    "ci:compile": "./node_modules/.bin/webpack --config webpack.config.js --bail",
    "ci:eslint": "./node_modules/.bin/eslint . --config .eslintrc",
    "build:compile": "./node_modules/.bin/webpack --config webpack.config.js --env production",
    "build:eslint": "./node_modules/.bin/eslint . --config .eslintrc",
    "compile": "webpack --config webpack.config.js --progress --colors",
    "lint": "eslint . --config .eslintrc --cache",
    "dev": "webpack --config webpack.config.js --progress --colors --watch",
    "package:lw": "build -lw --ia32 --x64",
    "package:l": "build -l --ia32 --x64",
    "package:w": "build -w --ia32 --x64",
    "package:m": "build -m"
  },
  "build": {
    "appId": "io.museeks.app",
    "electronVersion": "1.6.8",
    "files": [
      "src/*",
      "src/dist/**/*",
      "src/images/**/*",
      "src/main-process/**/*",
      "node_modules/teeny-conf/**/*"
    ],
    "mac": {
      "category": "public.app-category.music",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "AudioVideo",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "executableName": "museeks"
    },
    "win": {
      "target": "nsis",
      "icon": "src/images/logos/museeks.ico"
    }
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "bootstrap-css-only": "3.3.7",
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "globby": "6.1.0",
    "keymirror": "0.1.1",
    "level-js": "2.2.4",
    "linvodb3": "3.25.0",
    "musicmetadata": "2.0.5",
    "open-sans-fontface": "1.4.0",
    "queue": "4.2.1",
    "react": "15.4.2",
    "react-bootstrap": "0.30.10",
    "react-dom": "15.4.2",
    "react-fontawesome": "1.6.1",
    "react-keybinding-component": "0.4.1",
    "react-redux": "5.0.4",
    "react-router": "3.0.2",
    "react-router-bootstrap": "0.23.1",
    "react-simple-input": "0.3.1",
    "redux": "3.6.0",
    "semver": "5.3.0",
    "svg-inline-react": "1.0.2",
    "teeny-conf": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.24.1",
    "babili-webpack-plugin": "0.0.11",
    "css-loader": "0.28.0",
    "electron-builder": "^17.10.0",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "install": "0.8.8",
    "json-loader": "0.5.4",
    "minimist": "1.2.0",
    "node-sass": "4.5.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "svg-inline-loader": "0.7.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.4"
  }
}
